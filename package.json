{
  "name": "vaco-boilerplate-react-redux",
  "version": "0.1.0",
  "description": "Boilerplate for V. Alexander multi-page applications featuring ReactJS with Redux, Webpack with hot module reloading and Mocha/Chai/Enzyme for testing!",
  "main": "node bin/server.dev.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VACO-GitHub/vaco-boilerplate-react-redux"
  },
  "scripts": {
    "start": "node bin/server.dev.js",
    "dev": "node bin/server.dev.js",
    "dev:api": "node bin/server.js",
    "dev:clean-mac": "rimraf ./*/**/.DS_STORE",
    "lint": "eslint src api",
    "build": "scripts/build.sh",
    "test": "mocha \"src/**/*.test.js\" --require babel-register --require test/setup.js",
    "test:html": "mocha \"src/**/*.test.js\" --require babel-register --require test/setup.js --reporter mocha-html-reporter > test-results.html",
    "test:webpack": "webpack-bundle-analyzer stats.json --help"
  },
